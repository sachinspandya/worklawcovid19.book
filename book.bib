doi = "10.1016/j.econlet.2020.109722",
url = "http://www.sciencedirect.com/science/article/pii/S0165176520304821",
author = "Lucas Finamor and Dana Scott",
keywords = "Unemployment insurance, Employment, COVID-19, CARES Act",
abstract = "We test whether changes in unemployment insurance (UI) benefit generosity under the CARES Act in the US are associated with differential employment outcomes under the distinct conditions of the pandemic. While we observe a negative association between UI generosity and employment, we show that the relative employment gap arises before the Act was instituted, decreases in magnitude when the augmented benefits were in place, and does not change when the benefits expansion expires."
}

